# menentukan nama produk beserta nama merk dan juga harga nya
nama_produk = "boneka beruang "
merk_produk = " kiko"
harga_produk = "200.000"
hasil = "produk " + nama_produk + "dengan merk" + merk_produk + " dengan harga ori " + harga_produk
print("masukkan kesimpulan produk tersebut = ", hasil)

